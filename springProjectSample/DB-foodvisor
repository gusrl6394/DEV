 CREATE TABLE "FOODVISOR_REVIEW" 
(	"SEQ" NUMBER, 
	"TITLE" VARCHAR2(255 BYTE), 
	"ADDRESS" VARCHAR2(255 BYTE), 
	"IMG" VARCHAR2(1000 BYTE), 
	"CONTENT" VARCHAR2(1000 BYTE), 
	"WRITER" VARCHAR2(255 BYTE), 
	"REGDATE" DATE, 
	"GRADE" NUMBER DEFAULT 0, 
	"REVIEWCNT" NUMBER DEFAULT 0, 
	"LIKECNT" NUMBER DEFAULT 0, 
	"PWD" VARCHAR2(255 BYTE) DEFAULT 0000
);
   
CREATE SEQUENCE  "FOODVISOR_REVIEW_SEQ"  MINVALUE 1 MAXVALUE 9999999 INCREMENT BY 1 START WITH 1 NOCACHE  NOORDER  NOCYCLE ;
    